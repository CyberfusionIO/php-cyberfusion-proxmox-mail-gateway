{
    "name": "ywatchman/proxmox-mgw",
    "description": "Proxmox Mail Gateway laravel library",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "lib-curl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^5.8|6.0|^7.0|^8.0",
        "ext-json": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "YWatchman\\ProxmoxMGW\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YWatchman\\ProxmoxMGW\\Tests\\": "tests/"
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Yvan Watchman",
            "email": "ywatchman@cyberfusion.nl"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.0"
    }
}
