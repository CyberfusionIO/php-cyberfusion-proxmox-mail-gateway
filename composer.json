{
    "name": "cyberfusion/proxmox-mail-gateway",
    "description": "Proxmox Mail Gateway laravel library",
    "type": "library",
    "require": {
        "php": "^8.01|^8.1",
        "ext-json": "*",
        "lib-curl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "YWatchman\\ProxmoxMGW\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YWatchman\\ProxmoxMGW\\Tests\\": "tests/"
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Yvan Watchman",
            "email": "ywatchman@cyberfusion.nl"
        }
    ],
    "minimum-stability": "stable"
}
