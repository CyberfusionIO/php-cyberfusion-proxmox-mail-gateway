    "type": "library",
    "require": {
        "php": ">=7.1",
        "lib-curl": "*",
        "guzzlehttp/guzzle": "~5.3",
        "illuminate/support": "^5.8|6.0|^7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "YWatchman\\ProxmoxMGW\\": "src/"
